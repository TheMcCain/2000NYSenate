credits = function() {
    c = ["Coding\nTheMcCain\n\nWriting\nTheMcCain\n\nBanner, candidate images, and other art assets\nlittleamericanman\n\nBorrows code from 2012 Shattered,    \n\n Thanks for playing!"]
    text = ""
    for (i in c) {
        text += c[i] + "\n"
    }
    alert(text)
  }
 
campaignTrail_temp.credits = "<button onclick='credits()'>Various People</button>";

